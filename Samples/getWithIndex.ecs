
// `get` with indexes

// Pick a single character
//       0123456789012
set(str "Hello, world!")
print(
	"Lo" get(str 5)
	" <-- Lo,"
)

// Pick multiple characters, concats into one string
print(
	get(str 7 8 9 11 12)
	"  <-- 'word!'"
)

// Invalid indices are ignored
print( '"' get( str -1 0 100 ) '"  <-- "H"' )
